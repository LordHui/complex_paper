{
  "__name__": "Example experiment manifest for MusicNet",
  "device": "cuda:3",
  "threshold": -0.5,
  "datasets": {
    "musicnet-train": {
      "cls": "<class 'cplxpaper.musicnet.dataset.MusicNetRAM'>",
      "filename": "./data/musicnet_11khz_train.h5",
      "window": 4096,
      "stride": 1
    },
    "musicnet-train-512": {
      "cls": "<class 'cplxpaper.musicnet.dataset.MusicNetRAM'>",
      "filename": "./data/musicnet_11khz_train.h5",
      "window": 4096,
      "stride": 512
    },
    "musicnet-valid-128": {
      "cls": "<class 'cplxpaper.musicnet.dataset.MusicNetRAM'>",
      "filename": "./data/musicnet_11khz_valid.h5",
      "window": 4096,
      "stride": 128
    },
    "musicnet-test-128": {
      "cls": "<class 'cplxpaper.musicnet.dataset.MusicNetRAM'>",
      "filename": "./data/musicnet_11khz_test.h5",
      "window": 4096,
      "stride": 128
    }
  },
  "features": {
    "cls": "<class 'cplxpaper.auto.feeds.FeedFourierFeatures'>",
    "signal_ndim": 1,
    "cplx": true,
    "shift": true
  },
  "feeds": {
    "train": {
      "cls": "<class 'cplxpaper.musicnet.dataset.MusicNetDataLoader'>",
      "dataset": "musicnet-train",
      "pin_memory": true,
      "n_batches": 1000
    },
    "valid-768": {
      "cls": "<class 'torch.utils.data.dataloader.DataLoader'>",
      "dataset": "musicnet-valid-128",
      "batch_size": 768,
      "pin_memory": true,
      "shuffle": false,
      "n_batches": -1
    },
    "test-256": {
      "cls": "<class 'torch.utils.data.dataloader.DataLoader'>",
      "dataset": "musicnet-test-128",
      "batch_size": 256,
      "pin_memory": true,
      "shuffle": false,
      "n_batches": -1
    }
  },
  "scorers": {
    "validation": {
      "cls": "<class 'cplxpaper.musicnet.performance.MusicNetBasePerformance'>",
      "feed": "valid-768",
      "curves": false
    },
    "test": {
      "cls": "<class 'cplxpaper.musicnet.performance.MusicNetBasePerformance'>",
      "feed": "test-256",
      "curves": true
    }
  },
  "objective_terms": {
    "loss": {
      "cls": "<class 'torch.nn.modules.loss.BCEWithLogitsLoss'>",
      "reduction": "mean",
      "pos_weight": null
    },
    "kl_div": {
      "cls": "<class 'cplxpaper.auto.objective.ARDPenaltyObjective'>",
      "reduction": "mean",
      "coef": 1.0
    }
  },
  "model": {
    "cls": "<class 'cplxpaper.musicnet.complex.CplxDeepConvNet'>",
    "n_seq": 4096,
    "n_channels": 1,
    "n_outputs": 84
  },
  "stages": {
    "dense": {
      "snapshot": null,
      "feed": "train",
      "restart": true,
      "reset": false,
      "n_epochs": 200,
      "grad_clip": 0.05,
      "model" : {},
      "lr_scheduler": {
        "cls": "<class 'cplxpaper.musicnet.trabelsi2017.base.Trabelsi2017LRSchedule'>"
      },
      "optimizer": {
        "cls": "<class 'torch.optim.adam.Adam'>",
        "lr": 0.001,
        "betas": [
          0.9,
          0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0,
        "amsgrad": false
      },
      "objective": {
        "loss": 1.0,
        "kl_div": 0.0
      },
      "early": {
        "cls": "<class 'cplxpaper.musicnet.performance.PooledAveragePrecisionEarlyStopper'>",
        "scorer": "validation",
        "patience": 20,
        "cooldown": 0,
        "rtol": 0,
        "atol": 2e-2,
        "raises": "<class 'StopIteration'>"
      }
    },
    "sparsify": {
      "snapshot": null,
      "feed": "train",
      "restart": true,
      "reset": false,
      "n_epochs": 200,
      "grad_clip": 0.05,
      "model" : {
        "cls": "<class 'cplxpaper.musicnet.complex.extensions.CplxDeepConvNetARD'>"
      },
      "lr_scheduler": {
        "cls": "<class 'cplxpaper.musicnet.trabelsi2017.base.Trabelsi2017LRSchedule'>"
      },
      "optimizer": {
        "cls": "<class 'torch.optim.adam.Adam'>",
        "lr": 0.001,
        "betas": [
          0.9,
          0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0,
        "amsgrad": false
      },
      "objective": {
        "loss": 1.0,
        "kl_div": 1e-3
      },
      "early": null
    },
    "fine-tune": {
      "snapshot": null,
      "feed": "train",
      "restart": true,
      "reset": false,
      "n_epochs": 200,
      "grad_clip": 0.05,
      "model" : {
        "cls": "<class 'cplxpaper.musicnet.complex.extensions.CplxDeepConvNetMasked'>"
      },
      "lr_scheduler": {
        "cls": "<class 'cplxpaper.musicnet.trabelsi2017.base.Trabelsi2017LRSchedule'>"
      },
      "optimizer": {
        "cls": "<class 'torch.optim.adam.Adam'>",
        "lr": 0.001,
        "betas": [
          0.9,
          0.999
        ],
        "eps": 1e-08,
        "weight_decay": 0,
        "amsgrad": false
      },
      "objective": {
        "loss": 1.0,
        "kl_div": 0.0
      },
      "early": {
        "cls": "<class 'cplxpaper.musicnet.performance.PooledAveragePrecisionEarlyStopper'>",
        "scorer": "validation",
        "patience": 20,
        "cooldown": 0,
        "rtol": 0,
        "atol": 2e-2,
        "raises": "<class 'StopIteration'>"
      }
    }
  },
  "stage-order": [
    "dense",
    "sparsify",
    "fine-tune"
  ]
}